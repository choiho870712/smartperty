<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="wrap_content"
android:layout_marginBottom="1dp"
app:cardElevation="1dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/text_object_list_title"
            style="@style/text_normal_black_20sp"
            android:layout_width="0dp"
            android:layout_marginStart="16dp"
            android:singleLine="true"
            android:text="text_object_list_title"
            app:layout_constraintBottom_toTopOf="@+id/text_object_list_address"
            app:layout_constraintEnd_toStartOf="@+id/guideline17"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_object_list_address"
            style="@style/text_normal_black_14sp"
            android:layout_width="0dp"
            android:layout_marginStart="16dp"
            android:singleLine="true"
            android:text="text_object_list_address"
            app:layout_constraintBottom_toTopOf="@+id/text_object_list_information"
            app:layout_constraintEnd_toStartOf="@+id/guideline17"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_object_list_title" />

        <TextView
            android:id="@+id/text_object_list_information"
            style="@style/text_normal_black_14sp"
            android:layout_width="wrap_content"
            android:text="@string/squareFtUnit"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/card_image"
            app:layout_constraintTop_toBottomOf="@+id/card_image" />

        <TextView
            android:id="@+id/text_object_list_square_ft"
            style="@style/text_normal_black_14sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:text="30"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_object_list_information"
            app:layout_constraintEnd_toStartOf="@+id/text_object_list_information" />

        <TextView
            android:id="@+id/text_object_list_rent"
            style="@style/text_normal_black_14sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:text="15000"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_object_list_square_ft"
            app:layout_constraintEnd_toStartOf="@+id/text_object_list_square_ft" />

        <TextView
            android:id="@+id/text_object_list_information4"
            style="@style/text_normal_black_14sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/moneyUnit"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_object_list_rent"
            app:layout_constraintEnd_toStartOf="@+id/text_object_list_rent" />

        <TextView
            android:id="@+id/text_object_list_tenant_name"
            style="@style/text_normal_black_14sp"
            android:layout_width="0dp"
            android:layout_marginStart="16dp"
            android:singleLine="true"
            android:text="text_object_list_tenant_name"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/guideline17"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_object_list_address" />

        <androidx.cardview.widget.CardView
            android:id="@+id/card_image"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_margin="8dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="2dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/image_object_list"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:contentDescription="@string/houseImageDescription"
                    android:scaleType="centerCrop"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/empty_house" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>

        <Button
            android:id="@+id/button_choose_tenant"
            style="@style/button_rectangle_primary_hollow"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            android:text="查看資訊"
            app:layout_constraintBottom_toTopOf="@+id/text_object_list_rent"
            app:layout_constraintEnd_toStartOf="@+id/card_image"
            app:layout_constraintStart_toStartOf="@+id/guideline17"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.57177615" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>
