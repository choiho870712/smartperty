<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="1dp"
    app:cardElevation="1dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/text_object_folder_title"
            style="@style/text_normal_black_20sp"
            android:layout_width="0dp"
            android:layout_marginStart="16dp"
            android:text="大樓A"
            app:layout_constraintBottom_toTopOf="@+id/text_object_folder_square_ft"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_object_folder_square_ft"
            style="@style/text_normal_black_14sp"
            android:layout_width="0dp"
            android:layout_marginStart="16dp"
            android:text="130"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_object_folder_title" />

        <TextView
            android:id="@+id/text_object_folder_information2"
            style="@style/text_normal_black_14sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="坪"
            app:layout_constraintBottom_toBottomOf="@+id/text_object_folder_square_ft"
            app:layout_constraintStart_toEndOf="@+id/text_object_folder_square_ft"
            app:layout_constraintTop_toTopOf="@+id/text_object_folder_square_ft" />

        <TextView
            android:id="@+id/text_object_folder_item_number"
            style="@style/text_normal_black_14sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="5"
            app:layout_constraintBottom_toBottomOf="@+id/text_object_folder_square_ft"
            app:layout_constraintStart_toEndOf="@+id/text_object_folder_information2"
            app:layout_constraintTop_toTopOf="@+id/text_object_folder_square_ft" />

        <TextView
            android:id="@+id/text_object_folder_information4"
            style="@style/text_normal_black_14sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="戶"
            app:layout_constraintBottom_toBottomOf="@+id/text_object_folder_square_ft"
            app:layout_constraintStart_toEndOf="@+id/text_object_folder_item_number"
            app:layout_constraintTop_toTopOf="@+id/text_object_folder_square_ft" />

        <TextView
            android:id="@+id/text_object_folder_rent_rate_111"
            style="@style/text_normal_black_14sp"
            android:layout_width="0dp"
            android:layout_marginEnd="8dp"
            android:text="%"
            app:layout_constraintBottom_toBottomOf="@+id/text_object_folder_rent_rate3"
            app:layout_constraintEnd_toStartOf="@+id/card_image"
            app:layout_constraintTop_toTopOf="@+id/text_object_folder_rent_rate3" />

        <TextView
            android:id="@+id/text_object_folder_rent_rate"
            style="@style/text_normal_black_14sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="66"
            app:layout_constraintBottom_toBottomOf="@+id/text_object_folder_rent_rate3"
            app:layout_constraintEnd_toStartOf="@+id/text_object_folder_rent_rate_111"
            app:layout_constraintTop_toTopOf="@+id/text_object_folder_rent_rate3" />

        <TextView
            android:id="@+id/text_object_folder_rent_rate3"
            style="@style/text_normal_black_14sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="出租率"
            app:layout_constraintEnd_toStartOf="@+id/text_object_folder_rent_rate"
            app:layout_constraintTop_toTopOf="@+id/text_object_folder_title" />

        <androidx.cardview.widget.CardView
            android:id="@+id/card_image"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="8dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/image_object_folder"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>
