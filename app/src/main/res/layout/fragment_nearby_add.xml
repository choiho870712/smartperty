<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tenant.home.attractionsNearby.NearbyAddFragment">

    <TextView
        android:id="@+id/textView71"
        style="@style/text_normal_black_16sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="景點名稱"
        app:layout_constraintBottom_toTopOf="@+id/textView77"
        app:layout_constraintEnd_toStartOf="@+id/guideline19"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edit_create_property_name"
        style="@style/edit_text_normal_black_16sp_single_line"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline19"
        app:layout_constraintTop_toTopOf="@+id/textView71" />

    <EditText
        android:id="@+id/edit_create_property_full_address"
        style="@style/edit_text_normal_black_16sp_single_line"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/button_create_property_select_street"
        app:layout_constraintStart_toStartOf="@+id/button_create_property_select_street"
        app:layout_constraintTop_toTopOf="@+id/textView93" />

    <EditText
        android:id="@+id/edit_create_property_description"
        style="@style/edit_text_normal_black_16sp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:lines="3"
        android:maxLines="3"
        android:singleLine="false"
        app:layout_constraintEnd_toEndOf="@+id/edit_create_property_full_address"
        app:layout_constraintStart_toStartOf="@+id/edit_create_property_full_address"
        app:layout_constraintTop_toTopOf="@+id/textView98" />

    <TextView
        android:id="@+id/textView77"
        style="@style/text_normal_black_16sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="地區"
        app:layout_constraintBottom_toTopOf="@+id/textView91"
        app:layout_constraintEnd_toEndOf="@+id/textView71"
        app:layout_constraintTop_toBottomOf="@+id/textView71" />

    <TextView
        android:id="@+id/textView91"
        style="@style/text_normal_black_16sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="路名"
        app:layout_constraintBottom_toTopOf="@+id/textView92"
        app:layout_constraintEnd_toEndOf="@+id/textView77"
        app:layout_constraintTop_toBottomOf="@+id/textView77" />

    <TextView
        android:id="@+id/textView92"
        style="@style/text_normal_black_16sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="街名"
        app:layout_constraintBottom_toTopOf="@+id/textView93"
        app:layout_constraintEnd_toEndOf="@+id/textView91"
        app:layout_constraintTop_toBottomOf="@+id/textView91" />

    <TextView
        android:id="@+id/textView93"
        style="@style/text_normal_black_16sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="完整地址"
        app:layout_constraintBottom_toTopOf="@+id/textView98"
        app:layout_constraintEnd_toEndOf="@+id/textView92"
        app:layout_constraintTop_toBottomOf="@+id/textView92" />

    <TextView
        android:id="@+id/textView98"
        style="@style/text_normal_black_16sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="說明"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView93"
        app:layout_constraintTop_toBottomOf="@+id/textView93" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.3" />

    <Button
        android:id="@+id/button_create_property_select_region"
        style="@style/button_rectangle_primary_hollow"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:text="點擊選單"
        app:layout_constraintEnd_toEndOf="@+id/edit_create_property_name"
        app:layout_constraintStart_toStartOf="@+id/edit_create_property_name"
        app:layout_constraintTop_toTopOf="@+id/textView77" />

    <Button
        android:id="@+id/button_create_property_select_road"
        style="@style/button_rectangle_primary_hollow"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:text="點擊選單"
        app:layout_constraintEnd_toEndOf="@+id/button_create_property_select_region"
        app:layout_constraintStart_toStartOf="@+id/button_create_property_select_region"
        app:layout_constraintTop_toTopOf="@+id/textView91" />

    <Button
        android:id="@+id/button_create_property_select_street"
        style="@style/button_rectangle_primary_hollow"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:text="點擊選單"
        app:layout_constraintEnd_toEndOf="@+id/button_create_property_select_road"
        app:layout_constraintStart_toStartOf="@+id/button_create_property_select_road"
        app:layout_constraintTop_toTopOf="@+id/textView92" />

</androidx.constraintlayout.widget.ConstraintLayout>