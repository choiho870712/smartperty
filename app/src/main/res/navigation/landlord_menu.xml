<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/landlord_menu"
    app:startDestination="@id/landlordMenuFragment">

    <fragment
        android:id="@+id/landlordMenuFragment"
        android:name="com.smartperty.smartperty.landlord.menu.LandlordMenuFragment"
        android:label="@string/landlord_title_menu"
        tools:layout="@layout/landlord_fragment_menu_main">
        <action
            android:id="@+id/action_landlordMenuFragment_to_landlordObjectFolderFragment"
            app:destination="@id/estateDirectoryFragment" />
        <action
            android:id="@+id/action_landlordMenuFragment_to_landlordContractFragment"
            app:destination="@id/landlordContractFragment" />
        <action
            android:id="@+id/action_landlordMenuFragment_to_repairFragment"
            app:destination="@id/repairListFragment" />
        <action
            android:id="@+id/action_landlordMenuFragment_to_landlordPersonnelListFragment"
            app:destination="@id/landlordPersonnelListFragment" />
        <action
            android:id="@+id/action_landlordMenuFragment_to_landlordDataAnalysisFragment"
            app:destination="@id/landlordDataAnalysisFragment" />
    </fragment>
    <fragment
        android:id="@+id/estateFragment"
        android:name="com.smartperty.smartperty.landlord.menu.object.estate.EstateFragment"
        android:label="@string/landlord_title_object_item"
        tools:layout="@layout/fragment_estate" >
        <action
            android:id="@+id/action_landlordObjectFragment_to_landlordObjectItemEditFragment"
            app:destination="@id/etateEditFragment"
            app:popUpTo="@id/estateFragment" />
        <action
            android:id="@+id/action_estateFragment_to_repairOrderFragment"
            app:destination="@id/repairOrderFragment" />
    </fragment>
    <fragment
        android:id="@+id/estateDirectoryFragment"
        android:name="com.smartperty.smartperty.landlord.menu.object.estateDirectory.EstateDirectoryFragment"
        android:label="@string/landlord_title_object_folder"
        tools:layout="@layout/fragment_estate_directory">
        <action
            android:id="@+id/action_landlordObjectFolderFragment_to_landlordObjectListFragment"
            app:destination="@id/estateListFragment" />
        <action
            android:id="@+id/action_estateDirectoryFragment_to_estateDirectoryCreateFragment"
            app:destination="@id/estateDirectoryCreateFragment" />
    </fragment>
    <fragment
        android:id="@+id/estateListFragment"
        android:name="com.smartperty.smartperty.landlord.menu.object.estateList.EstateListFragment"
        android:label="@string/landlord_title_object_list"
        tools:layout="@layout/fragment_estate_list">
        <action
            android:id="@+id/action_landlordObjectListFragment_to_landlordObjectItemFragment"
            app:destination="@id/estateFragment" />
        <action
            android:id="@+id/action_landlordObjectListFragment_to_landlordObjectListAddFragment"
            app:destination="@id/estateCreateFragment" />
    </fragment>
    <fragment
        android:id="@+id/landlordContractFragment"
        android:name="com.smartperty.smartperty.landlord.menu.contract.LandlordContractFragment"
        android:label="@string/landlord_title_contract"
        tools:layout="@layout/landlord_fragment_contract">
        <action
            android:id="@+id/action_landlordContractFragment_to_landlordObjectItemFragment"
            app:destination="@id/estateFragment" />
    </fragment>
    <fragment
        android:id="@+id/repairListFragment"
        android:name="com.smartperty.smartperty.repair.RepairListFragment"
        android:label="@string/landlord_title_repair"
        tools:layout="@layout/fragment_recycler" >
        <action
            android:id="@+id/action_repairListFragment_to_repairOrderFragment"
            app:destination="@id/repairOrderFragment" />
        <action
            android:id="@+id/action_repairListFragment_to_repairOrderCreateFragment"
            app:destination="@id/repairOrderCreateFragment" />
    </fragment>
    <fragment
        android:id="@+id/landlordPersonnelListFragment"
        android:name="com.smartperty.smartperty.landlord.menu.personnel.PersonnelListFragment"
        android:label="@string/landlord_title_personnel"
        tools:layout="@layout/fragment_personnel_list" >
        <action
            android:id="@+id/action_personnelListFragment_to_personnelFragment"
            app:destination="@id/landlordPersonnelItemFragment" />
    </fragment>
    <fragment
        android:id="@+id/landlordDataAnalysisFragment"
        android:name="com.smartperty.smartperty.landlord.menu.dataAnalysis.LandlordDataAnalysisFragment"
        android:label="@string/landlord_title_data_analysis"
        tools:layout="@layout/landlord_fragment_data_analysis" />
    <fragment
        android:id="@+id/estateDirectoryCreateFragment"
        android:name="com.smartperty.smartperty.landlord.menu.object.estateDirectory.EstateDirectroyCreateFragment"
        android:label="新增物件"
        tools:layout="@layout/fragment_estate_directory_create" />
    <fragment
        android:id="@+id/estateCreateFragment"
        android:name="com.smartperty.smartperty.landlord.menu.object.estateList.EstateListCreateFragment"
        android:label="新增單位"
        tools:layout="@layout/fragment_estate_list_create"/>
    <fragment
        android:id="@+id/etateEditFragment"
        android:name="com.smartperty.smartperty.landlord.menu.object.estate.EstateEditFragment"
        android:label="編輯"
        tools:layout="@layout/fragment_estate_edit"/>
    <fragment
        android:id="@+id/landlordPersonnelItemFragment"
        android:name="com.smartperty.smartperty.landlord.menu.personnel.PersonnelFragment"
        android:label="landlord_fragment_personnel_item"
        tools:layout="@layout/fragment_personnel" />
    <fragment
        android:id="@+id/repairOrderFragment"
        android:name="com.smartperty.smartperty.repair.RepairOrderFragment"
        android:label="報修單"
        tools:layout="@layout/fragment_repair_order" >
        <action
            android:id="@+id/action_repairOrderFragment_to_chooseTenantFragment"
            app:destination="@id/chooseTenantFragment" />
        <action
            android:id="@+id/action_repairOrderFragment_to_choosePlumberFragment"
            app:destination="@id/choosePlumberFragment" />
        <action
            android:id="@+id/action_repairOrderFragment_to_addMessageFragment"
            app:destination="@id/addMessageFragment" />
    </fragment>
    <fragment
        android:id="@+id/repairOrderCreateFragment"
        android:name="com.smartperty.smartperty.repair.RepairOrderCreateFragment"
        android:label="填寫報修單"
        tools:layout="@layout/fragment_repair_order_create" >
        <action
            android:id="@+id/action_repairOrderCreateFragment_to_chooseTenantFragment"
            app:destination="@id/chooseTenantFragment" />
    </fragment>
    <fragment
        android:id="@+id/chooseTenantFragment"
        android:name="com.smartperty.smartperty.repair.ChooseTenantFragment"
        android:label="選擇住戶"
        tools:layout="@layout/fragment_choose_tenant" >
        <action
            android:id="@+id/action_chooseTenantFragment_to_landlordObjectFragment"
            app:destination="@id/estateFragment" />
    </fragment>
    <fragment
        android:id="@+id/choosePlumberFragment"
        android:name="com.smartperty.smartperty.repair.ChoosePlumberFragment"
        android:label="選擇維修人員"
        tools:layout="@layout/fragment_choose_plumber" />
    <fragment
        android:id="@+id/addMessageFragment"
        android:name="com.smartperty.smartperty.repair.AddPostFragment"
        android:label="新增訊息"
        tools:layout="@layout/fragment_add_post"/>

</navigation>