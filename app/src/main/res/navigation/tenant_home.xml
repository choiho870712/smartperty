<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tenant_home"
    app:startDestination="@+id/tenantHomeFragment">

    <fragment
        android:id="@+id/tenantHomeFragment"
        android:name="com.example.smartperty.tenant.home.TenantHomeFragment"
        android:label="@string/tenant_title_home"
        tools:layout="@layout/tenant_fragment_home_main" >
        <action
            android:id="@+id/action_tenantHomeFragment_to_tenantHosingRulesFragment"
            app:destination="@id/tenantHosingRulesFragment" />
        <action
            android:id="@+id/action_tenantHomeFragment_to_tenantRentAndUtilitiesFragment"
            app:destination="@id/tenantRentAndUtilitiesFragment" />
        <action
            android:id="@+id/action_tenantHomeFragment_to_tenantEquipmentManualFragment"
            app:destination="@id/tenantEquipmentManualFragment" />
        <action
            android:id="@+id/action_tenantHomeFragment_to_tenantContractFragment"
            app:destination="@id/tenantContractFragment" />
        <action
            android:id="@+id/action_tenantHomeFragment_to_tenantAttractionsNearbyFragment"
            app:destination="@id/tenantAttractionsNearbyFragment" />
        <action
            android:id="@+id/action_tenantHomeFragment_to_tenantRepairFragment"
            app:destination="@id/tenantRepairFragment" />
    </fragment>
    <fragment
        android:id="@+id/tenantHosingRulesFragment"
        android:name="com.example.smartperty.tenant.home.hosingRules.TenantHosingRulesFragment"
        android:label="tenant_fragment_hosing_rules"
        tools:layout="@layout/tenant_fragment_hosing_rules" />
    <fragment
        android:id="@+id/tenantRentAndUtilitiesFragment"
        android:name="com.example.smartperty.tenant.home.rentAndUtilities.TenantRentAndUtilitiesFragment"
        android:label="TenantRentAndUtilitiesFragment"
        tools:layout="@layout/tenant_fragment_rent_and_utilities"/>
    <fragment
        android:id="@+id/tenantEquipmentManualFragment"
        android:name="com.example.smartperty.tenant.home.equipmentManual.TenantEquipmentManualFragment"
        android:label="TenantEquipmentManualFragment"
        tools:layout="@layout/tenant_fragment_equipment_manual">
        <action
            android:id="@+id/action_tenantEquipmentManualFragment_to_tenantEquipmentManualPdfFragment"
            app:destination="@id/tenantEquipmentManualPdfFragment" />
    </fragment>
    <fragment
        android:id="@+id/tenantContractFragment"
        android:name="com.example.smartperty.tenant.home.contract.TenantContractFragment"
        android:label="TenantContractFragment"
        tools:layout="@layout/tenant_fragment_contract" />
    <fragment
        android:id="@+id/tenantAttractionsNearbyFragment"
        android:name="com.example.smartperty.tenant.home.attractionsNearby.TenantAttractionsNearbyFragment"
        android:label="TenantAttractionsNearbyFragment"
        tools:layout="@layout/tenant_fragment_attractions_nearby"/>
    <fragment
        android:id="@+id/tenantRepairFragment"
        android:name="com.example.smartperty.tenant.home.repair.TenantRepairFragment"
        android:label="TenantRepairFragment"
        tools:layout="@layout/tenant_fragment_repair"/>
    <fragment
        android:id="@+id/tenantEquipmentManualPdfFragment"
        android:name="com.example.smartperty.tenant.home.equipmentManualPdf.TenantEquipmentManualPdfFragment"
        android:label="TenantEquipmentManualPdfFragment"
        tools:layout="@layout/tenant_fragment_equipment_manual_pdf"/>

</navigation>